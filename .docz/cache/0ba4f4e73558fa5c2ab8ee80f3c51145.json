    echo $row['email'] . \"\\\\t\";\n        echo $row['password'] . \"\\\\n\";\n    }\n}\ncatch(PDOException $e)\n    {\n    echo $sql . \"<br>\" . $e->getMessage();\n    }\n\n$conn = null;\n\n?>\n`}</code></pre>\n    <h2 {...{\n      \"id\": \"treehouse-query-challenge\"\n    }}>{`Treehouse Query Challenge`}</h2>\n    <pre><code parentName=\"pre\" {...{\n        \"className\": \"language-php\"\n      }}>{`<?php\ninclude \"helper.php\";\n\n/*\n * helper.php contains\n * $results->query(\"SELECT member_id, email, fullname, level FROM members\");\n */\n$user = $results->fetchAll(PDO::FETCH_ASSOC);\nforeach ($user as $key)\n{\n  send_offer(\n    $key['member_id'],\n    $key['email'],\n    $key['fullname'],\n    $key['level']);\n}\n?>\n`}</code></pre>\n    </MDXLayout>;\n}\n\nMDXContent.isMDXComponent = true;\n  "]}]}